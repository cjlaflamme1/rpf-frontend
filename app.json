{
  "expo": {
    "name": "rpf-frontend",
    "plugins": [
      [
        "expo-camera"
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "slug": "rpf-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rpf_icon.png",
    "backgroundColor": "#9DBDC6",
    "splash": {
      "image": "./assets/rbt_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#9DBDC6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bunderIdentifier": "com.nhclimbingpartners",
      "supportsTablet": false,
      "backgroundColor": "#9DBDC6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rpf_icon.png",
        "backgroundColor": "#9DBDC6"
      },
      "backgroundColor": "#9DBDC6"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
