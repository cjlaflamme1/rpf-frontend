{
  "expo": {
    "name": "rpf-frontend",
    "slug": "rpf-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rpf_icon.png",
    "backgroundColor": "#9DBDC6",
    "splash": {
      "image": "./assets/rbt_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#9DBDC6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#9DBDC6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rpf_icon.png",
        "backgroundColor": "#9DBDC6"
      },
      "backgroundColor": "#9DBDC6"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
